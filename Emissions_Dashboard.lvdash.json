{
  "datasets": [
    {
      "name": "6db9a5b9",
      "displayName": "Emissions_per_person",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE) / NULLIF(CAST(population AS DOUBLE), 0) AS Emissions_per_person\r\n",
        "FROM emmisions.default.emissions_data\r\n",
        "ORDER BY Emissions_per_person DESC\r\n"
      ]
    },
    {
      "name": "d63cdb38",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT latitude,\r\n",
        "       longitude,\r\n",
        "       `GHG emissions mtons CO2e` AS Emissions\r\n",
        "FROM emmisions.default.emissions_data"
      ]
    },
    {
      "name": "47734d5f",
      "displayName": "Total_Emissions_Per_State",
      "queryLines": [
        "SELECT state_abbr,\r\n",
        "       SUM(CAST(REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE)) AS Total_emissions\r\n",
        "FROM emmisions.default.emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY total_emissions DESC\r\n"
      ]
    },
    {
      "name": "931c7772",
      "displayName": "Top10",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE) AS Total_Emissions\r\n",
        "FROM emmisions.default.emissions_data\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emmisions",
      "schema": "default"
    }
  ],
